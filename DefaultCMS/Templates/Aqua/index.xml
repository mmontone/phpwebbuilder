<templates>
	<template class="ModuleComponent">
		<div class="page" align="center">
			<div class="header">
				<div class="companyname" align="left"><container id="siteName"/></div>
				<div id="landmark"  align="right" class="menu">
					<template class="CommandLink">
						<a href="#">
							<container id="linkName" />
						</a>
						|
					</template>
				</div>
			</div>
			<br />
			<div class="content">
				<template class="ReferenceBar"><div style="text-align:left;width:100%;"><template class="CommandLink"><span> &gt;&gt; <container id="linkName"/></span></template></div></template>
				<div class="leftpanel">
					<container id="navigationbar" />
					<container id="actionsbar" />
				</div>
				<div class="contenttext">
					<container id="body" />
				</div>
				<div class="footer">
					<br />
					PHPWebBuilder | Designed by
					<a href="http://www.winkhosting.com/">
						Wink Hosting
					</a>
					.
				</div>
			</div>
		</div>
	</template>
	<template class="NavigationMenu">
		<div class="table_top">
			<span class="title_panel">
				<container id="secName" />
			</span>
		</div>
		<div class="table_content">
			<div class="table_text">
				<template class="CommandLink">
					<a class="news_more">
						<container id="linkName" /><br />
					</a>
				</template>
			</div>
		</div>
		<div class="table_bottom">
			<img src="$templatesdirimages/table_bottom.jpg" width="204" height="23"
				border="0" alt="" />
		</div>
		<br />
	</template>
		<template class="ActionsMenu">
		<div class="table_top">
			<span class="title_panel">
				<container id="secName" />
			</span>
		</div>
		<div class="table_content">
			<div class="table_text">
				<template class="CommandLink">
					<a class="news_more">
						<container id="linkName" /><br />
					</a>
				</template>
			</div>
		</div>
		<div class="table_bottom">
			<img src="$templatesdirimages/table_bottom.jpg" width="204" height="23"
				border="0" alt="" />
		</div>
		<br />
	</template>
</templates>