<templates>
 	<template class="CollectionElementChooser">
 		<div>
		<container id="className" /> class
	 	<table id="objs"><tbody>
 			<tr>
				<template class="SortByFieldComponent" >
				<td>
				<container class="ActionLink" />
				</td>
				</template>
				<td>Select</td>
			</tr>
			<template class="PersistentObjectViewer">
	 			<tr>
					<template class="FieldValueComponent" >
						<td><container id="value"/></td>
					</template>
					<td><a href="#" id="select"><img class="operation" title="Select" src="$templatesdir/icons/stock_ok.png"/></a></td>
					<td><a href="#" id="viewer">
								<img class="operation" title="View"
									src="$templatesdir/icons/stock_properties.png" />
					</a></td>
				</tr>
			</template>
		</tbody></table>
		<a href="#" id="new"><img class="operation" title="New" src="$templatesdir/icons/stock_new.png"/></a>

		<a href="#" id="first"><img class="operation" title="First" src="$templatesdir/icons/stock_first.png"/></a>
		<a href="#" id="prev"><img class="operation" title="Previous" src="$templatesdir/icons/stock_left.png"/></a>
		<input type="text" id="firstElem" style="width:4em"/>
		<a href="#" id="next"><img class="operation" title="Next" src="$templatesdir/icons/stock_right.png"/></a>
		<a href="#" id="last"><img class="operation" title="Last" src="$templatesdir/icons/stock_last.png"/></a>
		<a href="#" id="refresh">refresh</a>
		Showing <input type="text" id="pSize" style="width:4em"/> per page. <container id="realSize"/> elements available.


	 	</div>
	</template>
	<template class="CompositeSelect">
		<container id="display" />
		<container id="nav" />
	</template>
</templates>