<templates>
	<template class="QuestionDialog">
		<table>
			<tbody>
				<tr>
					<td>
						<img
							src="$templatesdir/icons/Neu/48x48/status/dialog-question.png" />
					</td>
					<td>
						<div>
							<h3>
								<container id="question" />
							</h3>
						</div>
						<input type="button" id="yes" value="Yes" />
						<input type="button" id="no" value="No" />
					</td>
				</tr>
			</tbody>
		</table>
	</template>

	<template class="NotificationDialog">
		<table>
			<tbody>
				<tr>
					<td>
						<img
							src="$templatesdir/icons/Neu/48x48/status/dialog-information.png" />
					</td>
					<td>
						<div>
							<h3>
								<container id="notification"/>
							</h3>
						</div>
						<input type="button" id="accept" value="Accept" />
					</td>
				</tr>
			</tbody>
		</table>
	</template>

	<template class="ValidationErrorsDisplayer">
		<div>
			<h3><translated>Correct the following errors</translated>:</h3>
		</div>
		<template class="ErrorMessage">
			<div class="error_message">
				<container id="error_message"/>
			</div>
		</template>
	</template>

	<template class="PersistentObjectEditor">
		<container id="validation_errors" />
		<translated>Object of class</translated>
		<container id="className" />
		id:
		<container id="idN" />
		<table>
			<tbody>
				<template class="FieldValueComponent">
					<tr>
						<td>
							<container id="fieldName" />
						</td>
						<td>
							<container id="value" />
						</td>
					</tr>
				</template>
			</tbody>
		</table>
		<a href="#" id="save">
			<img class="operation" title="Save changes"
				src="$templatesdir/icons/stock_save.png" />
		</a>
		<a href="#" id="cancel">
			<img class="operation" title="Cancel"
				src="$templatesdir/icons/stock_cancel.png" />
		</a>
		<a href="#" id="delete">
			<img class="operation" title="Delete"
				src="$templatesdir/icons/stock_delete.png" />
		</a>
	</template>

	<template class="PersistentObjectViewer">
		<translated>Object of class</translated>
		<container id="className" />
		id:
		<container id="idN" />
		<table>
			<tbody>
				<template class="FieldValueComponent">
					<tr>
						<td>
							<container id="fieldName" />
						</td>
						<td>
							<container id="value" />
						</td>
					</tr>
				</template>
			</tbody>
		</table>
		<a href="#" id="delete">
			<img class="operation" title="Delete"
				src="$templatesdir/icons/stock_delete.png" />
		</a>
		<a href="#" id="goback">
			<img class="operation" title="Go back"
				src="$templatesdir/icons/stock_cancel.png" />
		</a>
	</template>

	<template class="CollectionViewer">
		<div>
			<translated>Class</translated>
			:
			<container id="className" />
			<table>
				<thead>
					<tr>
						<template class="CompositeWidget">
							<td>
								<container class="ActionLink" />
							</td>
						</template>
						<td>
							<translated>Edit</translated>
						</td>
						<td>
							<translated>Delete</translated>
						</td>
					</tr>
				</thead>
				<tbody id="objs">
					<template class="PersistentObjectViewer">
						<tr>
							<template class="FieldValueComponent">
								<td>
									<container id="value" />
								</td>
							</template>
							<!-- <td><a href="#" id="view"><img class="operation" title="View" src="$templatesdir/icons/stock_properties.png"/></a></td> -->
							<td>
								<a href="#" id="edit">
									<img class="operation" title="Edit"
										src="$templatesdir/icons/stock_edit.png" />
								</a>
							</td>
							<td>
								<a href="#" id="delete">
									<img class="operation"
										title="Delete" src="$templatesdir/icons/stock_delete.png" />
								</a>
							</td>
						</tr>
					</template>
				</tbody>
			</table>
			<a href="#" id="new">
				<img class="operation" title="New"
					src="$templatesdir/icons/stock_new.png" />
			</a>

			<a href="#" id="first">
				<img class="operation" title="First"
					src="$templatesdir/icons/stock_first.png" />
			</a>
			<a href="#" id="prev">
				<img class="operation" title="Previous"
					src="$templatesdir/icons/stock_left.png" />
			</a>
			<input type="text" id="firstElem" style="width:4em" />
			<a href="#" id="next">
				<img class="operation" title="Next"
					src="$templatesdir/icons/stock_right.png" />
			</a>
			<a href="#" id="last">
				<img class="operation" title="Last"
					src="$templatesdir/icons/stock_last.png" />
			</a>
			<br />
			<a href="#" id="refresh">
				<translated>refresh</translated>
			</a>
			<a href="#" id="filter">
				<img class="operation" title="Search"
					src="$templatesdir/icons/stock_search.png" />
			</a>
			<translated>Showing</translated>
			<input type="text" id="pSize" style="width:4em" />
			<translated>per page</translated>
			.
			<container id="realSize" />
			<translated>elements available</translated>
			.
		</div>
	</template>
</templates>