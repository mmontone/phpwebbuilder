<templates>
 	<template class="EditObjectComponent">
	 	Object of Class <container id="className"/> id: <container id="id"/>
	 	<table><tbody>
			<template class="Obj" >
	 			<tr>
					<td><container id="name"/></td>
					<td><container id="value"/></td>
				</tr>
			</template>
		</tbody></table>
		<a href="#" id="save"><img class="operation" title="Save changes" src="$templatesdir/icons/stock_save.png"/></a>
		<a href="#" id="cancel"><img class="operation" title="Cancel" src="$templatesdir/icons/stock_cancel.png"/></a>
	</template>
 	<template class="ShowCollectionComponent">
 		<div>
		<container id="className" /> class
	 	<table id="objs"><tbody>
 			<tr>
				<template class="Obj" >
				<td>
				<container class="Text" />
				</td>
				</template>
				<td>Edit</td>
				<td>Delete</td>
			</tr>
			<template class="ShowObjectComponent">
	 			<tr>
					<template class="Obj" >
						<td><container id="value"/></td>
					</template>
					<td><a href="#" id="edit"><img class="operation" title="Edit" src="$templatesdir/icons/stock_edit.png"/></a></td>
					<td><a href="#" id="delete"><img class="operation" title="Delete" src="$templatesdir/icons/stock_delete.png"/></a></td>
				</tr>
			</template>
		</tbody></table>
		<a href="#" id="new"><img class="operation" title="New" src="$templatesdir/icons/stock_new.png"/></a>

		<a href="#" id="first"><img class="operation" title="First" src="$templatesdir/icons/stock_first.png"/></a>
		<a href="#" id="prev"><img class="operation" title="Previous" src="$templatesdir/icons/stock_left.png"/></a>
		<container id="firstElem"/>
		<a href="#" id="next"><img class="operation" title="Next" src="$templatesdir/icons/stock_right.png"/></a>
		<a href="#" id="last"><img class="operation" title="Last" src="$templatesdir/icons/stock_last.png"/></a>
		Showing <input type="text" id="pSize" style="width:4em"/> of <container id="realSize"/> available.


	 	</div>
	</template>
</templates>