<templates>
	<template class="DefaultCMS">
		<container id="menu"/>
		<div class="Body">
			<container id="body"/>
		</div>
	</template>
 	<template class="Menu">
 		<div class="Menu">
		<container id="SiteName"/> CMS <br/>
		<translated>User</translated>: <container id="username"/><br/>
		<container id="refresh"/>
		<ul id="menus" class="MenuList">
			<template class="MenuSectionComponent">
				<container id="secName" />
				<template class="MenuItemComponent"><div>
						<template class="CommandLink">
						    <a><li><container id="linkName" /></li></a>
						 </template>
				</div></template>
			</template>
		</ul>
		</div>
	</template>

	<template class="Login">
 	<table><tbody>
		<tr><td colspan="2"><translated>Fill in the username and password</translated>:</td></tr>
		<tr><td><translated>Username</translated>:</td><td> <input type="text" id="comp_username" /></td></tr>
		<tr><td><translated>Password</translated>:</td><td> <input type="password" id="comp_password" /></td></tr>
		<tr><td><container id="status"/></td></tr>
		<!--  I don't know how to apply translation on the login button -->
		<tr><td></td><td><input type="button" id="login" value="Login"/></td></tr>
	</tbody></table>
	</template>
</templates>